{
  "name": "Desktop-Starter-Kit",
  "version": "1.0.0",
  "description": "A boilerplate for Electron, Material, Firebase.",
  "main": "main.js",
  "scripts": {
    "start": "electron -r babel-register .",
    "electron": "electron",
    "dev": "rimraf dist && gulp dev --type dev",
    "dev-watch": "rimraf dist && gulp dev-watch --type dev",
    "prod": "rimraf dist && gulp build --type prod",
    "lint": "gulp lint",
    "clean": "rimraf dist",
    "reset": "git reset --hard HEAD && git clean -df",
    "reinstall": "yarn cache clean && git clean -dfx -e '.*' && yarn install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Shyam-Chen/Desktop-Starter-Kit.git"
  },
  "keywords": [
    "Electron"
  ],
  "author": "Yen-cheng Chen, 陳彥澄",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Shyam-Chen/Desktop-Starter-Kit/issues"
  },
  "homepage": "https://github.com/Shyam-Chen/Desktop-Starter-Kit#readme",
  "babel": {
    "presets": [
      "latest"
    ]
  },
  "dependencies": {
    "babel-register": "^6.18.0",
    "core-js": "^2.4.1",
    "dialog-polyfill": "^0.4.5",
    "firebase": "3.6.3",
    "material-design-lite": "^1.3.0"
  },
  "devDependencies": {
    "babel-core": "^6.21.0",
    "babel-eslint": "^7.1.1",
    "babel-plugin-external-helpers": "^6.18.0",
    "babel-plugin-lodash": "^3.2.11",
    "babel-plugin-transform-function-bind": "^6.8.0",
    "babel-preset-latest": "^6.16.0",
    "browser-sync": "^2.18.5",
    "connect-history-api-fallback": "^1.3.0",
    "cssnano": "^3.9.1",
    "electron": "^1.4.13",
    "electron-winstaller": "^2.5.1",
    "express": "^4.14.0",
    "express-history-api-fallback": "^2.1.0",
    "firebase-tools": "^3.2.1",
    "gulp": "^3.9.1",
    "gulp-eslint": "^3.0.1",
    "gulp-htmlhint": "^0.3.1",
    "gulp-htmlmin": "^3.0.0",
    "gulp-notify": "^2.2.0",
    "gulp-plumber": "^1.1.0",
    "gulp-protractor": "^3.0.0",
    "gulp-sourcemaps": "^2.2.1",
    "gulp-stylelint": "^3.7.0",
    "gulp-util": "^3.0.7",
    "jasmine-core": "^2.5.2",
    "jasmine-spec-reporter": "^2.7.0",
    "karma": "^1.3.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-jasmine": "^1.1.0",
    "karma-mocha-reporter": "^2.2.1",
    "karma-phantomjs-launcher": "^1.0.2",
    "karma-rollup-plugin": "^0.2.4",
    "phantomjs-prebuilt": "^2.1.14",
    "postcss-comment": "^1.0.3",
    "postcss-cssnext": "^2.9.0",
    "postcss-import": "^9.0.0",
    "postcss-modules": "^0.5.2",
    "protractor": "^4.0.13",
    "require-dir": "^0.3.1",
    "rimraf": "^2.5.4",
    "rollup-plugin-babel": "^2.7.1",
    "rollup-plugin-commonjs": "^6.0.0",
    "rollup-plugin-html": "^0.2.1",
    "rollup-plugin-image": "^1.0.2",
